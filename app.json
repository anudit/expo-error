{
  "expo": {
    "name": "StickerSmash",
    "slug": "StickerSmash",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anuditnagar.StickerSmash"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8af50c58-ecfb-4b17-8f5b-96a75eab5793"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/8af50c58-ecfb-4b17-8f5b-96a75eab5793"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true,
            "extraProguardRules": "-keep class com.facebook.hermes.unicode.** { *; }\n-keep class com.facebook.jni.** { *; }\n-keep public class com.horcrux.svg.** {*;}\n-keep public class com.unistyles.** {*;}",
            "newArchEnabled": false,
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "minSdkVersion": 25,
            "buildToolsVersion": "34.0.0"
          },
          "ios": {
            "deploymentTarget": "15.0",
            "newArchEnabled": false,
            "networkInspector": false,
            "useFrameworks": "static" // Firebase fails otherwise.
          }
        }
      ]
    ]
  }
}
